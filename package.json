{
  "name": "@prnsml/mocha-junit-reporter",
  "version": "2.1.1",
  "description": "A JUnit reporter for mocha which supports Jenkins.",
  "main": "index.js",
  "scripts": {
    "ci": "npm run lint && npm run test",
    "lint": "eslint '*.js' 'test/**/*.js'",
    "test": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=spec",
    "tdd": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=min --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prnsml/mocha-junit-reporter.git",
    "original-url": "https://github.com/michaelleeallen/mocha-junit-reporter"
  },
  "keywords": [
    "mocha",
    "junit",
    "jenkins",
    "reporter"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-xml": "^0.3.0",
    "eslint": "^7.0.0",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2",
    "test-console": "^1.0.0",
    "xmllint": "^0.1.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "md5": "^2.1.0",
    "mkdirp": "~0.5.1",
    "strip-ansi": "^4.0.0",
    "xml": "^1.0.0"
  },
  "peerDependencies": {
    "mocha": ">=2.2.5"
  },
  "bugs": {
    "url": "https://github.com/prnsml/mocha-junit-reporter/issues"
  },
  "homepage": "https://github.com/prnsml/mocha-junit-reporter#readme",
  "directories": {
    "test": "test"
  },
  "author": "prnsml"
}
